    // loads next reinforcement group
    // in: s2 = number

    SetSymbol(case_01, 0)
    SetSymbol(case_02, 1)
    SetSymbol(case_03, 2)
    SetSymbol(case_04, 3)
    SetSymbol(case_05, 4)
    SetSymbol(case_06, 5)
    SetSymbol(case_07, 6)
    SetSymbol(case_08, 7)
    SetSymbol(case_09, 8)
    SetSymbol(case_10, 9)
    SetSymbol(case_11, 10)
    SetSymbol(case_12, 11)
    SetSymbol(case_13, 12)
    SetSymbol(case_14, 13)
    SetSymbol(case_15, 14)

    SVAL s1 1
    BEQ case_01 s2 s1

    SVAL s1 2
    BEQ case_02 s2 s1

    SVAL s1 3
    BEQ case_03 s2 s1

    SVAL s1 4
    BEQ case_04 s2 s1

    SVAL s1 5
    BEQ case_05 s2 s1

    SVAL s1 6
